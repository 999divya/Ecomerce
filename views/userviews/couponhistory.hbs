<div class="breadcrumbs">
  <div class="container">
    <div class="row align-items-center">
      <div class="col-lg-6 col-md-6 col-12">
        <div class="breadcrumbs-content">
          <h1 class="page-title">Coupon History</h1>
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-12">
        <ul class="breadcrumb-nav">
          <li><a href="/back-to-home"><i class="lni lni-home"></i> Home</a></li>
          <li><a href="/back-to-home">Shop</a></li>
          <li>Coupon</li>
          <li>Coupon History</li>
        </ul>
      </div>
    </div>
  </div>
</div>



<section class="checkout-wrapper section">

  <div class="container">

    <div class="row justify-content-center">

      <div class="col-lg-8">
        <div class="button mb-3">
          <a href="/show-coupondata" class="btn animate">Coupon details</a>
        </div>
        <div class="checkout-steps-form-style-1">
          <ul id="accordionExample">

            <li>
              <h6 class="title" data-bs-toggle="collapse" data-bs-target="#collapseThree" aria-expanded="true"
                aria-controls="collapseThree">Please find your coupon code history here </h6>
              <section class="checkout-steps-form-content collapse show" id="collapseThree"
                aria-labelledby="headingThree" data-bs-parent="#accordionExample">
      
              {{#each findcouponhistory}}
                <div class="row">

                  <div class="tab-content" id="nav-tabContent">
                    <div class="tab-pane fade show active" id="nav-grid" role="tabpanel" aria-labelledby="nav-grid-tab">
                      <div class="row">
                        <div class="col-lg-12 col-md-12 col-12">

                          <div class="single-product">
                            <div class="row align-items-center">
                              <div class="col-lg-4 col-md-4 col-12">
                                <div class="product-image">
                                  <img class="float-left" src="/userasset/images/logo/promo.WEBP" alt="">
                                  <div class="button">
                       
                                  </div>
                                </div>
                              </div>

                             
                              <div class="col-lg-8 col-md-8 col-12">
                     
                                <div class="product-info">

                                  <div class="total-payable">
                                    <input type="text" style="display:none;" id="firstname">

                                  <strong>Order date:   </strong> {{this.Orderdate}}<br>
                                    <strong>Coupon Code(INR):   </strong> {{this.couponcode}}<br>
                                      <strong>Amount before applying coupon(INR):   </strong> {{this.totalAmount}}<br>
                                   <strong>Coupon Percent(INR):   </strong> {{this.percentage}}<br>
                                  
                                    <strong>Discount Amount(INR):   </strong> {{this.discount}}<br>
                                     <strong>Amount after applying coupon(INR):   </strong> {{this.discountedAmt}}<br>
                                 <strong> Status:   </strong> {{this.status}}<br>
                                  </div>

                                </div>

                              </div>
                            

                            </div>
                          </div>
                        </div>
                      </div>
                    </div>

                  </div>
                </div>
  {{/each}}
              </section>

            </li>

          </ul>
        </div>
      </div>
    </div>
  </div>
</section>






